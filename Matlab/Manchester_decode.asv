%% Função definida:
% bits = os bits utilizados para o vetor codificado

function MD = Manchester_decode(manchester)
    N = length(manchester)/2;
    new_code = zeros(N,1);
    for ii=1:N
       if isequal([machester(ii) machester(ii+1)],[1 0])
           new_code(ii)=0;
       else
           new_code(ii)=1;
           new_code(ii+1)=0;
       end
    end
    MD = new_code;
end